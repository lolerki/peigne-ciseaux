<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Message" table="message">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objet" type="string" column="objet" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mail" type="string" column="mail" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="message" type="text" column="message" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

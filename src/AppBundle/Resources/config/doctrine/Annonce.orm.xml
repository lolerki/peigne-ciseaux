<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Annonce" table="annonce">
    <indexes>
      <index name="options_fk" columns="id_options"/>
      <index name="user_fk" columns="id_user"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="message" type="text" column="message" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prix" type="integer" column="prix" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idOptions" target-entity="Options" fetch="LAZY">
      <join-columns>
        <join-column name="id_options" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUser" target-entity="Utilisateurs" fetch="LAZY">
      <join-columns>
        <join-column name="id_user" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
